{
  "name": "react-here-maps",
  "version": "0.0.6",
  "description": "React.js HERE Maps component",
  "main": "release/main.js",
  "scripts": {
    "gulp:transpile": "gulp transpile",
    "gulp:pretest": "gulp pretest",
    "gulp:posttest": "gulp posttest",
    "install-peers": "npm install react react-dom react-addons-test-utils",
    "remove-peers": "npm uninstall react react-dom react-addons-test-utils",
    "pretest": "npm run --silent install-peers && npm run --silent gulp:transpile && npm --silent run gulp:pretest",
    "test": "npm run test:ci",
    "test:ci": "mocha --require test/setup.js --recursive",
    "posttest": "npm run --silent remove-peers && npm run --silent gulp:posttest",
    "prepublish": "gulp && gulp lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Josh-ES/react-here-maps.git"
  },
  "keywords": [
    "React.js",
    "React",
    "react-component",
    "react",
    "component",
    "here",
    "map",
    "maps",
    "heremap",
    "here-map",
    "heremaps",
    "here-maps",
    "here",
    "map",
    "here",
    "maps"
  ],
  "author": "Josh-ES",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Josh-ES/react-here-maps/issues"
  },
  "homepage": "https://github.com/Josh-ES/react-here-maps#readme",
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "enzyme": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-sass": "^2.3.2",
    "gulp-tslint": "^6.0.2",
    "gulp-typescript": "^2.13.6",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "sinon": "^1.17.5",
    "tsify": "^1.0.3",
    "tslint": "^3.14.0-dev.1",
    "tslint-react": "^1.0.0",
    "typescript": "^2.1.0-dev.20160822",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "lodash": "^4.15.0",
    "reflect.ownkeys": "^0.2.0"
